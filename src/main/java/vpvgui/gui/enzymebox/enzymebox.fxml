   <children>
      <Label fx:id="restrictionLabel" styleClass="popup-label" stylesheets="@enzymebox.css" text="Choose restriction enzyme" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
      <VBox fx:id="restrictionVBox" layoutY="43.0" prefHeight="357.0" prefWidth="292.0" styleClass="pane" stylesheets="@enzymebox.css" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="45.0" />
      <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButtonClicked" text="OK" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />
   </children>
</AnchorPane>
