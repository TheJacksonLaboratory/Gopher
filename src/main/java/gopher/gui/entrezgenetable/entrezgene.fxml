<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="550.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gopher.gui.entrezgenetable.EntrezGenePresenter">
    <children>
        <HBox alignment="CENTER" style="-fx-background-color: #e1c1c1;" VBox.vgrow="SOMETIMES">
            <children>
            <WebView fx:id="wview" minHeight="-Infinity" minWidth="-Infinity" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
        <HBox alignment="CENTER" VBox.vgrow="NEVER">
            <children>
            <Button minWidth="75.0" mnemonicParsing="false" onAction="#uploadGenes" stylesheets="@entrezgene.css" text="Upload">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </Button>
                <Button minWidth="75.0" mnemonicParsing="false" onAction="#validateGeneSymbols" stylesheets="@entrezgene.css" text="Validate">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin></Button>
            <Button alignment="CENTER" minWidth="75.0" mnemonicParsing="false" onAction="#acceptGenes" stylesheets="@entrezgene.css" text="Accept" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
