<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<ScrollPane fx:id="contentScrollPane" fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vpvgui.gui.viewpointpanel.ViewPointPresenter">
   <content>
      <VBox alignment="TOP_CENTER">
         <children>
            <WebView fx:id="ucscContentWebView" minHeight="-Infinity" minWidth="-Infinity" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
            <TableView fx:id="viewPointsTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="300.0" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="isSelectedTableColumn" editable="false" maxWidth="1.7976931348623157E308" prefWidth="90.0" text="Selected?" />
                <TableColumn fx:id="refSeqIdTableColumn" editable="false" maxWidth="1.7976931348623157E308" prefWidth="135.0" text="Refseq ID" />
                  <TableColumn fx:id="targetIdTableColumn" editable="false" maxWidth="1.7976931348623157E308" prefWidth="161.0" text="Target ID" />
                  <TableColumn fx:id="genomicPosTableColumn" editable="false" maxWidth="1.7976931348623157E308" prefWidth="223.0" text="Genomic Pos" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <AnchorPane minWidth="500.0" VBox.vgrow="NEVER">
               <children>
                  <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#refreshUCSCButtonAction" prefHeight="30.0" prefWidth="110.0" text="Refresh UCSC" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
                  <Button layoutX="140.0" layoutY="10.0" mnemonicParsing="false" onAction="#saveButtonAction" prefHeight="30.0" prefWidth="110.0" text="Save" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="130.0" AnchorPane.topAnchor="10.0" />
                  <Button layoutX="270.0" layoutY="10.0" mnemonicParsing="false" onAction="#closeButtonAction" prefHeight="30.0" prefWidth="110.0" text="Close" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </content>
</ScrollPane>
